/**
 * Light high contrast
 * @extends all functional tokens
 * @description All overwrites for functional color tokens for light high contrast color mode are in this file
 */
{
  fgColor: {
    muted: {
      $value: '{base.color.gray.9}',
      $type: 'color',
    },
    danger: {
      $value: '{base.color.red.6}',
      $type: 'color',
    },
    accent: {
      $value: '{base.color.blue.6}',
      $type: 'color',
    },
  },
  bgColor: {
    secondary: {
      $value: '{base.color.gray.1}',
      $type: 'color',
    },
    neutral: {
      muted: {
        $value: '{base.color.gray.1}',
        $type: 'color',
      },
    },
    disabled: {
      $value: '{base.color.gray.1}',
      $type: 'color',
    },
    success: {
      emphasis: {
        $value: '{base.color.green.5}',
        $type: 'color',
      },
    },
    attention: {
      emphasis: {
        $value: '{base.color.yellow.5}',
        $type: 'color',
      },
    },
  },
  borderColor: {
    default: {
      $value: '{base.color.gray.8}',
      $type: 'color',
    },
    secondary: {
      $value: '{base.color.gray.4}',
      $type: 'color',
    },
    decorative: {
      $value: '{borderColor.emphasis}',
      $type: 'color',
    },
    accent: {
      muted: {
        $value: '{base.color.blue.3}',
        $type: 'color',
      },
    },
    success: {
      muted: {
        $value: '{base.color.green.3}',
        $type: 'color',
      },
      emphasis: {
        $value: '{base.color.green.5}',
        $type: 'color',
      },
    },
    attention: {
      muted: {
        $value: '{base.color.yellow.3}',
        $type: 'color',
      },
      emphasis: {
        $value: '{base.color.yellow.5}',
        $type: 'color',
      },
    },
    severe: {
      muted: {
        $value: '{base.color.orange.3}',
        $type: 'color',
      },
    },
    danger: {
      muted: {
        $value: '{base.color.red.3}',
        $type: 'color',
      },
    },
    done: {
      muted: {
        $value: '{base.color.purple.3}',
        $type: 'color',
      },
    },
    sponsors: {
      muted: {
        $value: '{base.color.pink.3}',
        $type: 'color',
      },
    },
  },
  // patterns
  topicTag: {
    borderColor: {
      $value: '{borderColor.accent.emphasis}',
      $type: 'color',
    },
  },
  counter: {
    borderColor: {
      $value: '{borderColor.default}',
      $type: 'color',
    },
  },
  control: {
    bgColor: {
      rest: {
        $value: '{base.color.gray.1}',
        $type: 'color',
      },
      hover: {
        $value: '{base.color.gray.1}',
        $type: 'color',
        mix: {
          color: '{base.color.gray.3}',
          weight: 0.2,
        },
      },
      active: {
        $value: '{base.color.gray.1}',
        $type: 'color',
        mix: {
          color: '{base.color.gray.3}',
          weight: 0.4,
        },
      },
      selected: {
        $value: '{base.color.gray.3}',
        $type: 'color',
      },
    },
    borderColor: {
      emphasis: {
        $value: '{borderColor.default}',
        $type: 'color',
      },
    },
    transparent: {
      bgColor: {
        hover: {
          $value: '{base.color.gray.1}',
          $type: 'color',
        },
        active: {
          $value: '{base.color.gray.2}',
          $type: 'color',
        },
        selected: {
          $value: '{base.color.gray.2}',
          $type: 'color',
        },
      },
      borderColor: {
        hover: {
          $value: '{borderColor.default}',
          $type: 'color',
        },
      },
    },
    danger: {
      fgColor: {
        hover: {
          $value: '{fgColor.onEmphasis}',
          $type: 'color',
        },
      },
      bgColor: {
        hover: {
          $value: '{bgColor.danger.emphasis}',
          $type: 'color',
        },
        active: {
          $value: '{bgColor.danger.emphasis}',
          $type: 'color',
          mix: {
            color: '{base.color.red.7}',
            weight: 0.4,
          },
        },
      },
    },
  },
  controlTrack: {
    bgColor: {
      rest: {
        $value: '{base.color.gray.5}',
        $type: 'color',
      },
      hover: {
        $value: '{base.color.gray.5}',
        $type: 'color',
        mix: {
          color: '{base.color.gray.7}',
          weight: 0.2,
        },
      },
      active: {
        $value: '{base.color.gray.5}',
        $type: 'color',
        mix: {
          color: '{base.color.gray.7}',
          weight: 0.4,
        },
      },
      disabled: {
        $value: '{fgColor.disabled}',
        $type: 'color',
      },
    },
    fgColor: {
      rest: {
        $value: '{fgColor.onEmphasis}',
        $type: 'color',
      },
    },
  },
  button: {
    default: {
      borderColor: {
        rest: {
          $value: '{borderColor.default}',
          $type: 'color',
        },
      },
    },
    primary: {
      bgColor: {
        rest: {
          $value: '{base.color.green.5}',
          $type: 'color',
        },
        hover: {
          $value: '{base.color.green.5}',
          $type: 'color',
          mix: {
            color: '{base.color.green.7}',
            weight: 0.2,
          },
        },
        active: {
          $value: '{base.color.green.5}',
          $type: 'color',
          mix: {
            color: '{base.color.green.7}',
            weight: 0.4,
          },
        },
      },
      borderColor: {
        rest: {
          $value: '{base.color.green.7}',
          $type: 'color',
        },
        hover: {
          $value: '{base.color.green.7}',
          $type: 'color',
        },
      },
    },
  },
  diffBlob: {
    addition: {
      fgColor: {
        text: {
          $value: '{fgColor.onEmphasis}',
          $type: 'color',
        },
      },
      bgColor: {
        word: {
          $value: '{bgColor.success.emphasis}',
          $type: 'color',
        },
      },
    },
    deletion: {
      fgColor: {
        text: {
          $value: '{fgColor.onEmphasis}',
          $type: 'color',
        },
      },
      bgColor: {
        word: {
          $value: '{bgColor.danger.emphasis}',
          $type: 'color',
        },
      },
    },
    hunk: {
      bgColor: {
        num: {
          $value: '{base.color.blue.1}',
          $type: 'color',
        },
      },
    },
  },
  header: {
    divider: {
      borderColor: {
        $value: '{base.color.gray.3}',
        $type: 'color',
      },
    },
  },
}
