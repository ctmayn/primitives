{
  shadow: {
    insetHighlight: {
      $value: {
        color: '{base.color.white}',
        alpha: 0.07,
        offsetX: '0px',
        offsetY: '0px',
        blur: '0px',
        spread: '1px',
        inset: true
      },
      $type: 'shadow'
    },
    inset: {
      $value: [{
        color: '{base.color.black}',
        alpha: 1,
        offsetX: '0px',
        offsetY: '1px',
        blur: '0px',
        spread: '0px',
        inset: true
      },{
        color: '{base.color.white}',
        alpha: 0.07,
        offsetX: '0px',
        offsetY: '1px',
        blur: '0px',
        spread: '0px',
        inset: false
      }],
      $type: 'shadow'
    },
    highlight: {
      $value: {
        color: '{base.color.white}',
        alpha: 0.25,
        offsetX: '0px',
        offsetY: '1px',
        blur: '0px',
        spread: '0px',
        inset: true
      },
      $type: 'shadow'
    },
    resting: {
      xsmall: {
        $value: [{
            color: '{base.color.black}',
            alpha: 0.64,
            offsetX: '0px',
            offsetY: '1px',
            blur: '0px',
            spread: '0px',
            inset: false
          },
          '{shadow.insetHighlight}'
        ],
        $type: 'shadow'
      },
      small: {
        $value: [
          {
            color: '{base.color.black}',
            alpha: 0.6,
            offsetX: '0px',
            offsetY: '1px',
            blur: '0px',
            spread: '0px',
            inset: false
          },
          {
            color: '{base.color.black}',
            alpha: 0.64,
            offsetX: '0px',
            offsetY: '2px',
            blur: '4px',
            spread: '0px',
            inset: false
          },
          '{shadow.insetHighlight}'
        ],
        $type: 'shadow'
      },
      medium: {
        $value: [
          {
            color: '{base.color.black}',
            alpha: 0.4,
            offsetX: '0px',
            offsetY: '1px',
            blur: '2px',
            spread: '0px',
            inset: false
          },
          {
            color: '{base.color.black}',
            alpha: 0.48,
            offsetX: '0px',
            offsetY: '2px',
            blur: '4px',
            spread: '0px',
            inset: false
          },
          {
            color: '{base.color.black}',
            alpha: 0.56,
            offsetX: '0px',
            offsetY: '3px',
            blur: '6px',
            spread: '0px',
            inset: false
          },
          '{shadow.insetHighlight}'
        ],
        $type: 'shadow'
      }
    }
  }
}
