{
  "base": {
    "viewportRange": {
      "narrow": {
        "value": "(max-width: calc({base.breakpoint.medium} - 0.02px))"
      },
      "narrowLandscape": {
        "value": "(max-width: calc({base.breakpoint.large} - 0.02px) and (max-height: calc({base.breakpoint.small} - 0.02px)) and (orientation: landscape))"
      },
      "regular": {
        "value": "(min-width: {base.breakpoint.medium})"
      },
      "wide": {
        "value": "(min-width: {base.breakpoint.xxlarge})"
      },
      "portrait": {
        "value": "(orientation: portrait)"
      },
      "landscape": {
        "value": "(orientation: landscape)"
      }
    }
  }
}
